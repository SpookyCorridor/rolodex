<h2>Libraries</h2>

<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>id</th>
            <th>email</th>
			<th>message</th>
		</tr>
	</thead>
	<tbody>
		{{#each model as |contact| }}
			<tr>
                <th>{{contact.id}}</th>
				<th>{{contact.email}}</th>
				<td>{{contact.message}}</td>
			</tr>
		{{/each}}
	</tbody>
</table>