<h2>Libraries</h2>

<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>id</th>
			<th>E-mail</th>
			<th>Phone #</th>
		</tr>
	</thead>
	<tbody>
		{{#each model as |library| }}
			<tr>
				<th>{{library.name}}</th>
				<td>{{library.address}}</td>
				<td>{{library.phone}}</td>
			</tr>
		{{/each}}
	</tbody>
</table>